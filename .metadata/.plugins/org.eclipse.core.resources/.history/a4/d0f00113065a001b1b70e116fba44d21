package com.cos.interexam.domain;

import java.util.ArrayList;
import java.util.List;

import com.cos.exam.domain.Movie;

@Reposito
public class MovieRepository {
	
	public List<Movie> findAll() {
		List<Movie> movies = new ArrayList<>();
		
		movies.add(new Movie(1, "타이틀1", 4.5, "2021-01-19"));
		movies.add(new Movie(2, "타이틀2", 3.8, "2021-01-29"));
		movies.add(new Movie(3, "타이틀3", 4.9, "2021-02-09"));
		
		return movies;
	}
}
